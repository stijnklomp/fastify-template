{
	"dependencies": {
		"@fastify/autoload": "^5.8.0",
		"@fastify/helmet": "^11.1.1",
		"@fastify/sensible": "^5.5.0",
		"fastify": "^4.26.2",
		"fastify-cli": "^6.1.1",
		"fastify-plugin": "^4.5.1"
	},
	"description": "",
	"devDependencies": {
		"@stylistic/eslint-plugin": "^1.7.0",
		"@types/jest": "^29.5.12",
		"@types/node": "^20.12.4",
		"@typescript-eslint/eslint-plugin": "^7.5.0",
		"@typescript-eslint/parser": "^7.5.0",
		"concurrently": "^8.2.2",
		"eslint": "^8.57.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-airbnb-typescript": "^18.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-jest": "^27.9.0",
		"eslint-plugin-jsonc": "^2.15.0",
		"eslint-plugin-prettier": "^5.1.3",
		"husky": "^9.0.11",
		"jest": "^29.7.0",
		"jest-junit": "^16.0.0",
		"jsonc-eslint-parser": "^2.4.0",
		"prettier": "^3.2.5",
		"stijnklomp-linting-formatting-config": "^6.8.0",
		"ts-jest": "^29.1.2",
		"ts-node": "^10.9.2",
		"typedoc": "^0.25.12",
		"typescript": "^5.4.4"
	},
	"directories": {
		"test": "test"
	},
	"license": "MIT",
	"main": "app.ts",
	"name": "fastify-template",
	"private": false,
	"scripts": {
		"build": "rm -rf dist ; tsc",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start": "fastify start -l info dist/app.js",
		"dev:start": "fastify start --ignore-watch=.ts$ -w -l info -P dist/app.js",
		"dev": "npm run build && npm run start",
		"dev:watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:build -- -w\" \"(npm run build && npm run dev:start)\"",
		"lint": "eslint .",
		"test": "jest .",
		"test:coverage": "npm run test -- --coverage",
		"ci:test:coverage": "npm run test:coverage -- --coverageReporters=cobertura --passWithNoTests",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/jest/bin/jest --runInBand",
		"prepare": "husky install"
	},
	"type": "module",
	"version": "1.0.0"
}
