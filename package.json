{
	"name": "fastify-template",
	"version": "1.0.0",
	"private": false,
	"description": "",
	"license": "MIT",
	"type": "module",
	"main": "app.ts",
	"directories": {
		"test": "test"
	},
	"scripts": {
		"afterTest": "rm -rf test/temp-coverage && mkdir -p test/temp-coverage && npm run stage-coverage-jsons && npm run generate-coverage-all-packages && rm -rf test/temp-coverage",
		"build": "rm -rf dist ; node esbuild.js",
		"ci:test:coverage": "npm run test:coverage -- --coverageReporters=cobertura",
		"dev": "rm -rf dist ; node esbuild.js dev",
		"doc": "typedoc",
		"generate-coverage-all-packages": "nyc report --reporter=html --reporter=text --reporter=lcov --reporter=clover --report-dir=test/combined-coverage --temp-dir=test/temp-coverage",
		"integrity": "./scripts/integrity.sh",
		"lint": "eslint --report-unused-disable-directives . --cache",
		"lint:fix": "npm run sort ; eslint --report-unused-disable-directives . --cache --fix",
		"migrate": "npx prisma migrate deploy",
		"prepare": "husky",
		"sort": "./node_modules/stijnklomp-linting-formatting-config/sortJson.sh './test/tsconfig.json'",
		"stage-coverage-jsons": "cp test/unit/coverage/coverage-final.json test/temp-coverage/unit.json && cp test/feature/coverage/coverage-final.json test/temp-coverage/feature.json",
		"start": "node dist/app.js",
		"test": "npm run test:unit",
		"test:acceptance": "ENV=test LOGS=test USE_ELASTIC_APM=false jest --config test/acceptance/jest.config.js",
		"test:acceptance:coverage": "npm run test:acceptance -- --coverage",
		"test:coverage": "npm run test:unit:coverage ; npm run test:feature:coverage ; npm run afterTest",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/jest/bin/jest --runInBand",
		"test:feature": "NODE_OPTIONS=--experimental-vm-modules ENV=test LOGS=test jest --config test/feature/jest.config.js --passWithNoTests",
		"test:feature:coverage": "npm run test:feature -- --coverage",
		"test:unit": "NODE_OPTIONS=--experimental-vm-modules ENV=test LOGS=test USE_ELASTIC_APM=false jest --config test/unit/jest.config.js",
		"test:unit:coverage": "npm run test:unit -- --coverage"
	},
	"dependencies": {
		"@fastify/cors": "^11.2.0",
		"@fastify/helmet": "^13.0.2",
		"@fastify/sensible": "^6.0.4",
		"@fastify/type-provider-typebox": "^6.1.0",
		"@opentelemetry/api": "^1.9.0",
		"@prisma/adapter-pg": "^7.2.0",
		"@prisma/client": "^7.2.0",
		"@sinclair/typebox": "^0.34.47",
		"ajv": "^8.17.1",
		"ajv-formats": "^3.0.1",
		"amqplib": "^0.10.9",
		"dotenv": "^17.2.3",
		"elastic-apm-node": "^4.15.0",
		"fastify": "^5.7.1",
		"fastify-plugin": "^5.1.0",
		"fluent-json-schema": "^6.0.0",
		"globals": "^17.0.0",
		"hyperid": "^3.3.0",
		"nyc": "^17.1.0",
		"pg": "^8.17.1",
		"readable-stream": "^4.7.0",
		"redis": "^5.10.0",
		"stijnklomp-linting-formatting-config": "^7.0.4"
	},
	"devDependencies": {
		"@es-exec/esbuild-plugin-serve": "^0.0.5",
		"@eslint/compat": "^2.0.1",
		"@eslint/js": "^9.39.2",
		"@fastify/autoload": "^6.3.1",
		"@fastify/swagger": "^9.6.1",
		"@types/amqplib": "^0.10.8",
		"@types/jest": "^30.0.0",
		"@types/node": "^25.0.9",
		"esbuild": "^0.27.2",
		"eslint": "^9.39.2",
		"eslint-plugin-jest": "^29.12.1",
		"eslint-plugin-jsonc": "^2.21.0",
		"eslint-plugin-markdown": "^5.1.0",
		"eslint-plugin-prettier": "^5.5.5",
		"eslint-plugin-yml": "^1.19.1",
		"fastify-cli": "^7.4.1",
		"glob": "^13.0.0",
		"husky": "^9.1.7",
		"jest": "^30.2.0",
		"jest-extended": "^7.0.0",
		"jest-junit": "^16.0.0",
		"jest-mock-extended": "^4.0.0",
		"json-sort-cli": "^4.1.3",
		"lint-staged": "^16.2.7",
		"micromatch": "^4.0.8",
		"module": "^2.0.0",
		"prettier": "^3.8.0",
		"prettier-plugin-packagejson": "^2.5.21",
		"prisma": "^7.2.0",
		"ts-jest": "^29.4.6",
		"ts-node": "^10.9.2",
		"typedoc": "^0.28.16",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.53.0"
	},
	"engines": {
		"node": ">=18.18.0"
	}
}
